<Window x:Class="MISOTEN_APPLICATION.Screen.SystemSelect.SystemSelect_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MISOTEN_APPLICATION.Screen.SystemSelect"
        mc:Ignorable="d"
        Title="SystemSelect_Window" Height="600" Width="530" WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize">

    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="メニュー" Width="65">
                    <MenuItem Header="設定" Click="Button_Click"/>
                    <Separator/>
                    <MenuItem Header="開発者用" Click="DevelopSystemButton_Click"/>
                </MenuItem>
            </Menu>
            <StackPanel>
            </StackPanel>
        </DockPanel>
        <Frame x:Name="SystemSelectFrame" />
    </Grid>
</Window>
